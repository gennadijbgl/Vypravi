<resources>

<color name="card_color1">@color/md_deep_purple_400</color>
    <color name="spinner_color">@color/md_grey_800</color>
    <style name="AppCompatAlertDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">#FFCC00</item>
        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="android:background">#5fa3d0</item>
    </style>

<style name="PopupMenuMy" parent="Base.Widget.AppCompat.Light.PopupMenu">
    <item name="android:colorButtonNormal">#8b8b8b</item>
</style>

    <style name="PopupMenuMy1" >
        <item name="colorPrimary">@color/primary</item>
        <!-- colorPrimaryDark is used for the status bar -->
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <!-- colorAccent is used as the default value for colorControlActivated which is used to tint widgets -->


        <item name="android:textColor">#12c456</item>
        <item name="android:textColorPrimary">#02c4dd</item>
        <item name="colorAccent">#FFCC00</item>
        <item name="colorControlNormal">#e20d0d</item>
        <item name="colorControlHighlight">#3311a3b9</item>
        <item name="android:popupBackground">#e20d0d</item>
        <item name="android:textColorSecondary">#f7f270</item>


    </style>


    <!-- Base application theme. -->
    <style name="AppTheme" parent="MaterialDrawerTheme.TranslucentStatus">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>


        <item name="material_drawer_background">#F2F2F2</item>
        <item name="material_drawer_primary_text">#212121</item>
        <item name="material_drawer_secondary_text">#727272 </item>

        <item name="material_drawer_hint_text">#000000</item>
        <item name="material_drawer_divider">#B6B6B6</item>
        <item name="material_drawer_selected">@color/md_blue_500</item>
        <item name="material_drawer_selected_text">@color/md_black_1000</item>
        <item name="material_drawer_header_selection_text">#B6B6B6 </item>
        <item name="material_drawer_primary_icon">@color/md_black_1000</item>

        <!-- CAB :D -->
        <item name="windowActionModeOverlay">true</item>
        <item name="android:textColor">@color/md_white_1000</item>
        <item name="android:textColorSecondary">@color/md_black_1000</item>
        <item name="android:textColorHint">#8b8b8b</item>


        <item name="met_underlineColor">@color/md_black_1000</item>
        <item name="met_baseColor">@color/md_black_1000</item>
        <item name="met_primaryColor">@color/accent</item>
        <item name="met_textColor">@color/md_black_1000</item>
        <item name="met_textColorHint">#878787</item>
        <item name="met_errorColor">@color/colorPrimaryDark</item>

    </style>


    <style name="mytext" parent="AppTheme">

        <item name="material_drawer_background">#ff0000</item>
        <item name="material_drawer_primary_text">#212121</item>
        <item name="material_drawer_secondary_text">#ff0000</item>
        <item name="met_underlineColor">@color/md_black_1000</item>
        <item name="met_baseColor">@color/md_black_1000</item>
        <item name="met_primaryColor">@color/accent</item>
        <item name="met_textColor">@color/md_black_1000</item>
        <item name="met_textColorHint">#878787</item>
        <item name="met_errorColor">@color/colorPrimaryDark</item>
        <item name="met_floatingLabel">@color/colorPrimaryDark</item>

    </style>

